{
  "name": "DiscordBot-YappyGithub",
  "version": "2.0.0",
  "description": "A Github repo monitor bot for Discord",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "start": "node lib/index.js",
    "lint": "prettier --write 'lib/**/*.js' 'db/**/*.js'",
    "docs": "docgen --source lib --jsdoc .jsdoc.json --custom docs/index.yml --output docs/docs.json",
    "docs:test": "docgen --source lib --custom docs/index.yml",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex rollback"
  },
  "repository": {
    "url": "https://github.com/YappyBots/YappyGithub",
    "type": "git"
  },
  "author": "David Sevilla Martin <dsevilla192@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^16.28.7",
    "body-parser": "^1.19.0",
    "bookshelf": "^0.15.1",
    "bugsnag": "^2.4.3",
    "chalk": "^2.4.2",
    "clean-stack": "^2.2.0",
    "discord.js": "github:discordjs/discord.js",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "hbs": "^4.0.4",
    "jsondiffpatch": "^0.3.11",
    "knex": "^0.19.1",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^5.6.9",
    "p-queue": "^6.1.1",
    "performance-now": "^2.1.0",
    "pretty-error": "^2.1.1",
    "raven": "^2.6.4",
    "snekfetch": "^4.0.4",
    "sqlite3": "^4.0.9",
    "swag": "^0.7.0",
    "winston": "^3.2.1",
    "yappybots-addons": "github:YappyBots/yappy-addons"
  },
  "devDependencies": {
    "discord.js-docgen": "github:hydrabolt/discord.js-docgen",
    "jsdoc": "^3.6.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "prettier": "^1.18.2"
  }
}
